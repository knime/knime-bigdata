   <extension
         point="com.knime.bigdata.spark.TemplateRepository">
      <template-repository
            id="com.knime.bigdata.spark.node.scripting.java.util.SparkJavaSnippetFileTemplateRepositoryProvider"
            provider-class="com.knime.bigdata.spark.node.scripting.java.util.SparkJavaSnippetFileTemplateRepositoryProvider">
      </template-repository>
      <template-repository
            id="com.knime.bigdata.spark.node.scripting.java.util.SparkJavaSnippetPluginTemplateRepositoryProvider"
            provider-class="com.knime.bigdata.spark.node.scripting.java.util.SparkJavaSnippetPluginTemplateRepositoryProvider">
      </template-repository>
   </extension>
   <extension
         point="org.knime.core.PortType">
      <portType
            hidden="false"
            name="Spark Context"
            objectClass="com.knime.bigdata.spark.port.context.SparkContextPortObject"
            objectSerializer="com.knime.bigdata.spark.port.context.SparkContextPortObject$ModelSerializer"
            specClass="com.knime.bigdata.spark.port.context.SparkContextPortObjectSpec"
            specSerializer="com.knime.bigdata.spark.port.context.SparkContextPortObjectSpec$SpecSerializer">
      </portType>
      <portType
            color="#000000"
            hidden="false"
            name="Spark Data"
            objectClass="com.knime.bigdata.spark.port.data.SparkDataPortObject"
            objectSerializer="com.knime.bigdata.spark.port.data.SparkDataPortObject$Serializer"
            specClass="com.knime.bigdata.spark.port.data.SparkDataPortObjectSpec"
            specSerializer="com.knime.bigdata.spark.port.data.SparkDataPortObjectSpec$Serializer">
      </portType>
      <portType
            color="#7DBDFF"
            hidden="false"
            name="Spark Model"
            objectClass="com.knime.bigdata.spark.port.model.SparkModelPortObject"
            objectSerializer="com.knime.bigdata.spark.port.model.SparkModelPortObject$Serializer"
            specClass="com.knime.bigdata.spark.port.model.SparkModelPortObjectSpec"
            specSerializer="com.knime.bigdata.spark.port.model.SparkModelPortObjectSpec$Serializer">
      </portType>
   </extension>
</plugin>
