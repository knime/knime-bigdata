      <node
            category-path="/toolintegration/spark/mining/clustering"
            factory-class="com.knime.bigdata.spark.node.mllib.clustering.kmeans.MLlibKMeansNodeFactory">
      </node>
      <node
            category-path="/toolintegration/spark/mining/clustering"
            factory-class="com.knime.bigdata.spark.node.mllib.clustering.assigner.MLlibClusterAssignerNodeFactory"
            >
      </node>
      <node
            category-path="/toolintegration/spark/mining/prediction"
            factory-class="com.knime.bigdata.spark.node.mllib.prediction.decisiontree.MLlibDecisionTreeNodeFactory"
            >
      </node>
      <node
            category-path="/toolintegration/spark/mining/pmml"
            factory-class="com.knime.bigdata.spark.node.pmml.converter.MLlib2PMMLNodeFactory"
            >
      </node>
      <node
            category-path="/toolintegration/spark/mining/pmml"
            factory-class="com.knime.bigdata.spark.node.pmml.predictor.compiled.SparkPMMLPredictorNodeFactory"
            >
      </node>
      <node
            category-path="/toolintegration/spark/io/db"
            factory-class="com.knime.bigdata.spark.node.io.hive.reader.Hive2SparkNodeFactory"
