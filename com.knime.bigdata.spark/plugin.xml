      </node>
      <node
            category-path="/toolintegration/spark/column/convert"
            factory-class="com.knime.bigdata.spark.node.pmml.transformation.compiling.SparkTransformationPMMLApplyNodeFactory"
            >
      </node>
<!--      <node
            category-path="/toolintegration/spark/column/convert"
            factory-class="com.knime.bigdata.spark.node.pmml.transformation.compiled.SparkCompiledTransformationPMMLApplyNodeFactory">
      </node>
      -->
