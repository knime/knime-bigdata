<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="SparkTypeConverter" name="SparkTypeConverter" schema="schema/SparkTypeConverter.exsd"/>
   <extension-point id="TemplateRepository" name="Spark Java Snippet Template Repository" schema="schema/TemplateRepository.exsd"/>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.knime.bigdata.spark.preferences.SparkPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.knime.workbench.ui.preferences"
            class="com.knime.bigdata.spark.preferences.SparkPreferencePage"
            id="com.knime.bigdata.spark.preferences.SparkPreferencePage"
            name="Spark">
      </page>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/category.png"
            level-id="spark"
            locked="true"
            name="Spark"
            path="/toolintegration">
      </category>
      <category
            icon="icons/category_reader.png"
            level-id="io"
            locked="true"
            name="IO"
            path="/toolintegration/spark">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="convert"
            locked="true"
            name="Convert &amp; Replace"
            path="/toolintegration/spark/column">
      </category>
      <category
            icon="icons/category_mining.png"
            level-id="mining"
            locked="true"
            name="Mining"
            path="/toolintegration/spark">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="split"
            locked="true"
            name="Split &amp; Combine"
            path="/toolintegration/spark/column">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="scoring"
            locked="true"
            name="Scoring"
            path="/toolintegration/spark/mining">
      </category>
      <category
            icon="icons/category_misc.png"
            level-id="misc"
            locked="true"
            name="Misc"
            path="/toolintegration/spark">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="java"
            locked="true"
            name="Java Snippet"
            path="/toolintegration/spark/misc">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="transform"
            locked="true"
            name="Transform"
            path="/toolintegration/spark/row">
      </category>
      <category
            icon="icons/row.png"
            level-id="row"
            locked="true"
            name="Row"
            path="/toolintegration/spark">
      </category>
      <category
            after="io"
            icon="icons/column.png"
            level-id="column"
            locked="true"
            name="Column"
            path="/toolintegration/spark">
      </category>
      <category
            icon="icons/read.png"
            level-id="read"
            locked="true"
            name="Read"
            path="/toolintegration/spark/io">
      </category>
      <category
            icon="icons/write.png"
            level-id="write"
            locked="true"
            name="Write"
            path="/toolintegration/spark/io">
      </category>
      <category
            icon="icons/category_database.png"
            level-id="db"
            locked="true"
            name="Database"
            path="/toolintegration/spark/io">
      </category>
      <category
            icon="icons/category_statistics.png"
            level-id="statistics"
            locked="true"
            name="Statistics"
            path="/toolintegration/spark">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="clustering"
            locked="true"
            name="Clustering"
            path="/toolintegration/spark/mining">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="prediction"
            locked="true"
            name="Prediction"
            path="/toolintegration/spark/mining">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="pmml"
            locked="true"
            name="PMML"
            path="/toolintegration/spark/mining">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="reduction"
            locked="true"
            name="Dimensionality Reduction"
            path="/toolintegration/spark/mining">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="management"
            locked="true"
            name="Management"
            path="/toolintegration/spark/misc">
      </category>
   </extension>
   <extension
