      </node>
      <node
            category-path="/labs/spark"
            expert-flag="false"
            factory-class="com.knime.bigdata.spark.node.mllib.pmml.converter.MLlib2PMMLNodeFactory"
            id="com.knime.bigdata.spark.node.mllib.pmml.converter.MLlib2PMMLNodeFactory">
      </node>
      <node
            category-path="/labs/spark"
            expert-flag="false"
            factory-class="com.knime.bigdata.spark.node.mllib.pmml.predictor.MLlibPMMLPredictorNodeFactory"
            id="com.knime.bigdata.spark.node.mllib.pmml.predictor.MLlibPMMLPredictorNodeFactory">
      </node>
   </extension>
</plugin>
