<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="com.knime.bigdata.spark.SparkTypeConverter" name="SparkTypeConverter" schema="schema/SparkTypeConverter.exsd"/>
   <extension-point id="com.knime.bigdata.spark.TemplateRepository" name="Spark Java Snippet Template Repository" schema="schema/TemplateRepository.exsd"/>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/category.png"
            level-id="spark"
            locked="true"
            name="Spark"
            path="/labs">
      </category>
      <category
            icon="icons/category_io.png"
            level-id="io"
            locked="true"
            name="IO"
            path="/labs/spark">
      </category>
      <category
            after="io"
            icon="icons/missing.gif"
            level-id="convert"
            locked="true"
            name="Convert &amp; Replace"
            path="/labs/spark">
      </category>
      <category
            icon="icons/category_learner.gif"
            level-id="mining"
            locked="true"
            name="Mining"
            path="/labs/spark">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="split"
            locked="true"
            name="Split &amp; Combine"
            path="/labs/spark">
      </category>
      <category
            icon="icons/category_misc.png"
            level-id="misc"
            locked="true"
            name="Misc"
            path="/labs/spark">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="java"
            locked="true"
            name="Java Snippet"
            path="/labs/spark/misc">
      </category>
   </extension>
   <extension
         point="org.knime.workbench.repository.nodes">
