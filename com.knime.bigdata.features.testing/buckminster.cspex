<?xml version="1.0" encoding="UTF-8"?>
<cspecExtension xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bc="http://www.eclipse.org/buckminster/Common-1.0" xmlns="http://www.eclipse.org/buckminster/CSpec-1.0">
  <dependencies>
    <dependency name="com.knime.features.explorer.sharedspace" componentType="eclipse.feature"/>
    <dependency name="com.knime.features.reportbatch" componentType="eclipse.feature"/>
    <dependency name="com.knime.features.reporting.designer" componentType="eclipse.feature"/>
    <dependency name="com.knime.features.reporting.runtime" componentType="eclipse.feature"/>
    <dependency name="org.knime.features.testingapplication" componentType="eclipse.feature"/>
    <!--  these dependencies are needed for for the automatic build system -->
    <dependency name="org.knime.build.config" componentType="osgi.bundle" />
  </dependencies>
  <actions>
    <!-- Disable generation of source feature -->
    <public name="source.feature.jars" actor="null"/>
    <!-- Disable generation of source bundles -->
    <public name="source.bundle.jars" actor="null"/>
  </actions>
  <alterActions>
    <remove name="source.feature.jars"/>
    <remove name="source.bundle.jars"/>
  </alterActions>
</cspecExtension>
